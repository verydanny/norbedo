        "@sveltejs/enhanced-img": "^0.8.4",
        "@sveltejs/kit": "^2.48.0",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.16",
        "@types/node": "^24.9.1",
        "@vitest/browser": "^4.0.3",
        "daisyui": "^5.3.10",
        "eslint": "^9.38.0",
